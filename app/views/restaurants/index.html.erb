<h1>Restaurants</h1>

<ul class="list-group">
  <% @restaurants.each do |restaurant| %>
    <li class="list-group-item"><%= link_to restaurant.name, restaurant_path(restaurant) %> </li>
  <% end %>
</ul>

<br>
<br>
<%= link_to "Add a new restaurant", new_restaurant_path, class: "btn btn-primary" %>
